{
    "name": "menu",
    "label": "â””Menu",
    "label_singular": "Menu",
    "description": "Maintain Site Navigation",
    "folder": "/content/menu",
    "extension": "md",
    "format": "frontmatter",
    "create": true,
    "delete": true,
    "identifier_field": "slug",
    "slug": "{{fields.title}}",
    "summary": "{{fields.title}}",
    "fields": [
        {
            "name": "title",
            "label": "Title",
            "widget": "string",
            "required": true
        },
        {
            "label": "Menu Entries",
            "label_singular": "Menu Entries",
            "name": "menu_entries",
            "widget": "list",
            "fields": [
                {
                    "label": "Label",
                    "name": "label",
                    "widget": "string"
                },
                {
                    "label": "Link",
                    "name": "slug",
                    "widget": "string"
                },
                {
                    "label": "Icon Style",
                    "name": "iconstyle",
                    "widget": "select",
                    "options": ["outline", "solid"],
                    "multiple": false,
                    "required": true
                },
                {
                    "label": "HeroIcon",
                    "name": "iconname",
                    "widget": "heroicon"
                }
            ]
        }
    ]
}
